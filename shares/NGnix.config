NGnix config

sudo mkdir -p /var/www/foodjinn.com/public_html
sudo chown -R www-data:www-data /var/www/foodjinn.com/public_html
sudo vi /var/www/foodjinn.com/public_html/index.html

sudo cp /etc/nginx/sites-available/default /etc/nginx/sites-available/foodjinn.com

//
        server_name localhost;

        location / { try_files $uri @[app]; }
        location @[app] {
                uwsgi_pass 127.0.0.1:4242;
                include uwsgi_params;
        }

        location /static {
            root   /srv/www/foodjinn.com/public_html/static/;
            index  index.html index.htm;
        }

	server {
        listen   80; ## listen for ipv4; this line is default and implied
        #listen   [::]:80 default ipv6only=on; ## listen for ipv6

        root /var/www/example.com/public_html;
        index index.html index.htm;

        # Make site accessible from http://localhost/
        server_name example.com;
}
